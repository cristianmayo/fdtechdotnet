<section class="container page-content">
    <aside>
        <div class="page-content__toc">
            <h6>Jump to:</h6>
            <ul>
            {% for docContent in collections.aboutpage | sortedCollectionByFilenameAsc %}
                <li class="page-content__content-anchor">
                    <a href="#{{ docContent.data.title | slugify }}"> {{ docContent.data.title }}</a>
                </li>
            {% endfor %}
            </ul>
        </div>
    </aside>
    <div class="page-content__content">
        {% for docContent in collections.aboutpage | sortedCollectionByFilenameAsc %}
            <article class="page-content__content-entry">
                <h2>{{ docContent.data.title }}<a id="{{ docContent.data.title | slugify }}"> Â» </a></h2>
                {{ docContent.templateContent | safe }}
            </article>
        {% endfor %}
    </div>
</section>